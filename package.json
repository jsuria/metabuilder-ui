{
  "name": "platform-elastic-docs-ui-mbuilder",
  "version": "1.0.4",
  "description": "Metabuilder Application NPM Package",
  "author": "Miko Suria <js@irayaenergies.com>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-library": "vue-cli-service build --target lib --name platform-elastic-docs-ui-mbuilder ./src/install.js",
    "pub": "npm version patch --force && npm publish"
